{
  "name": "vue-maptiler-polygons",
  "version": "0.1.0",
  "description": "Reusable Vue 3 map component library built on MapTiler SDK with hierarchical polygon management, color-coded data visualization, and multi-map sync.",
  "type": "module",
  "main": "./dist/vue-maptiler-polygons.cjs",
  "module": "./dist/vue-maptiler-polygons.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-maptiler-polygons.mjs",
      "require": "./dist/vue-maptiler-polygons.cjs"
    },
    "./style.css": "./dist/vue-maptiler-polygons.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "@maptiler/sdk": "^2.0.0"
  },
  "devDependencies": {
    "vue": "^3.5.13",
    "@maptiler/sdk": "^2.4.0",
    "vite": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "typescript": "^5.7.0",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.2.0"
  },
  "license": "MIT"
}
